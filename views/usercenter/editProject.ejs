    <div class="module_dialog status_dialog" id="basic_infor">
        <div class="module_header">
            <button class="module_close f_fr">×</button>
            <h4 class="module_title">项目基本信息修改</h4>
        </div>
        <div class="module_body status_body">
            <div class="dialog_form cg_dialog">
                <form id="basic_infor_form">
                    <div class="item basic_item"><span>项目名称：</span>
                        <input type="text" class="validate[required]" name="projectName" id="projectName" value="<%-project.projectName!=null?project.projectName:'' %>">

                    </div>
                    <div class="item basic_item"><span>一句话介绍：</span>
                        <input type="text" class="validate[required]" name="shortIntroduce" id="shortIntroduce" value="<%-project.shortIntroduce!=null?project.shortIntroduce:''%>">
                    </div>
                    <div class="item basic_item clearfix">
                        <span class="f_fl">行业：</span>
                        <select id="industryCategory" name="industryCategory" class="validate[required] select_1 basic_select" onchange=getIndustry(this.id,"industry")>
                            <option value="">请选择</option>
                            <% industryList.forEach(function(item,index){ %>
                            <option  value=<%=item.sortId%> <%if(item.sortId==project.industryCategory){%>selected="selected"<%}%> > <%=item.industryName%> </option>
                            <% })%>
                        </select>
                        <select name="industry" class="validate[required] select_1 basic_select" id="industry">
                            <option value="">请选择</option>
                            <% industry.forEach(function(item,index){ %>
                            <option  value=<%=item.sortId%> <%if(item.sortId==project.industry){%>selected="selected"<%}%> > <%=item.industryName%> </option>
                            <% })%>
                        </select>
                    </div>
                    <div class="item basic_item clearfix">
                        <span class="f_fl">所在地：</span>
                         <select  name="province" id="province" class="select_1 basic_select validate[required]" onchange=getCity(this.id,"city")>
                            <option value="请选择">请选择</option>
                            <% provinces.forEach(function(item,index){ %>
                            <option  value=<%=item.id%> <%if(item.id==project.province){%>selected="selected"<%}%> > <%=item.areaName%> </option>
                            <% })%>
                        </select>
                        <select name="city" id="city" class="select_1 basic_select validate[required]" onchange=getCity(this.id,"county")>
                            <option value="请选择">请选择</option>
                            <% city.forEach(function(item,index){ %>
                            <option  value=<%=item.id%> <%if(item.id==project.city){%>selected="selected"<%}%> > <%=item.areaName%> </option>
                            <% })%>
                        </select>
                        <select name="county" id="county" class="select_1 basic_select validate[required]">
                            <option value="请选择">请选择</option>
                            <% county.forEach(function(item,index){ %>
                            <option  value=<%=item.id%> <%if(item.id==project.county){%>selected="selected"<%}%> > <%=item.areaName%> </option>
                            <% })%>
                        </select>
                    </div>
                    <div class="item basic_item"><span></span>
                        <!-- <%=project.projectname%> -->
                        <input type="text" name="address" class="validate[required]" id="address" value="<%-project.address!=null?project.address:''%>"><em>街道地址</em> </div>
                    <div class="error"></div>
                    <div class="dia_btns">
                        <button class="dia_submit" type="submit">提交</button>
                        <button class="cancel">取消</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
