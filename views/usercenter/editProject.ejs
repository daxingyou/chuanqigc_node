    <!--Begin 现有店铺概括-->
    <div class="module_dialog status_dialog" id="exist_shop">
        <div class="module_header">
            <button class="module_close f_fr">×</button>
            <h4 class="module_title">现有店铺概括</h4>
        </div>
        <div class="module_body exist_shop_body">
            <div class="dialog_form cg_dialog">
                <form id="saveShopsForm">
                   <%if (Shops.length!=0) {%>
                    <%Shops.forEach(function(item,index){%> 
                    <%if (index==0) {%>
                        <h1>第一家店铺：</h1>
                        <input type="hidden" value="<%=item.id%>" id="id_1">
                    <%}else if(index==1){%>
                         <h1>第二家店铺：</h1>
                         <input type="hidden" value="<%=item.id%>" id="id_2">
                    <%}else if(index==2){%>
                    <h1>第三家店铺：</h1>
                    <input type="hidden" value="<%=item.id%>" id="id_3">
                    <%}%>
                    <div class="item basic_item exist_item"><span>店铺名称：</span>
                        <input type="text" value="<%=item.shopname%>" name="shopname" id="shopname_<%=index+1%>" >
                    </div>                 
                    <div class="item basic_item exist_item"><span>店铺地址：</span>
                        <input type="text" name="shopaddress" value="<%=item.shopaddress%>" id="shopaddress_<%=index+1%>">
                    </div>
                    <div class="item basic_item exist_item"><span>店铺筹备总投资：</span>
                        <input type="text" value="<%=item.totalInvestment%>" class="validate[custom[number]]"  name="totalInvestment" id="totalInvestment_<%=index+1%>">&nbsp;元</div>
                    <div class="item basic_item exist_item"><span>从筹备到运营周期：</span>
                        <input type="text" value="<%=item.sumDays%>" class="validate[custom[number]]"  name="sumDays" >&nbsp;天</div>
                    <div class="item basic_item exist_item"><span>盈利可分红时间：</span>
                        <input type="text" value="<%=item.interestDays%>" class="validate[custom[number]]"  name="interestDays" id="interestDays_<%=index+1%>">&nbsp;天</div>
                    <div class="item basic_item exist_item"><span>店铺营业面积：</span>
                        <input type="text" value="<%=item.businessArea%>"  class="validate[custom[number]]"  name="businessArea" id="businessArea_<%=index+1%>" >&nbsp;平米</div>
                    <div class="item basic_item exist_item"><span>客流量：</span>
                        <input type="text" value="<%=item.clientFlow%>"  class="validate[custom[number]]" id="clientFlow_<%=index+1%>"  name="clientFlow">&nbsp;位/日</div>
                    <div class="item basic_item exist_item"><span>人均消费：</span>
                        <input type="text" id="averageSpend_<%=index+1%>"  value="<%=item.averageSpend%>" class="validate[custom[number]]"  name="averageSpend" >&nbsp;元</div>
                    <div class="item basic_item exist_item"><span>上年度营业收入：</span>
                        <input type="text" value="<%=item.lastyearIncome%>" class="validate[custom[number]]"  name="lastyearIncome" id="lastyearIncome_<%=index+1%>" >&nbsp;元</div>
