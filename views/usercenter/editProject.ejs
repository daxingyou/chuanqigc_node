           $("#projectInvestScale").val((Math.round((parseInt(projectAmount)/parseInt(totalAmount))*10000)/100).toFixed(2));//项目方所占比例
           $("#investorScale").val((Math.round(((parseInt(totalAmount)-parseInt(projectAmount))/parseInt(totalAmount))*10000)/100).toFixed(2))//投资方所占比例
       })
