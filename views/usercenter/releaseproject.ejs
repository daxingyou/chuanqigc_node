                        financialSupport=toArray(item.financialSupport);
                       myResources=toArray(item.myResources);
                     if (capabilitySupport.length!=0) {
                        $.each(capabilitySupport,function(index, el) {
                            strc+=" <li>"+el+"</li>";
                        });
                     }
                     if (financialSupport.length!=0) {
