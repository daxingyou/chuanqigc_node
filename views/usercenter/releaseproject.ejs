function confirmSubReview(projectId) {
       $.artDialog({
            title: '提示信息',
            height:50,
            width:250,
            content: '您确定要提交吗？',
            okValue: '确定',
            ok: function () {
                ycrediect("0","/usercenter/publishYet/subReview/"+projectId);
            },
            cancelValue: '取消',
            cancel: function () {
            }
        });
    }
