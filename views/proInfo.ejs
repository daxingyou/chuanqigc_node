    $(".comment_box").delegate(".reply_cancel","click",function(){
        $(this).parents(".replydiv").empty();
    })
    //点击提交，回复帖子
    $(".comment_box").delegate(".reply_submit","click",function(){
        var thisO=$(this);
        var content=$(this).prev("textarea").val();
        if(content==""){
            dialog("请输入回帖内容！");
            return false;
        }
        var data={
            "projectId":$("#projectId").val(),
            "content":content,
            "postId":$(this).attr("postId")
        }
        ajaxFunc(data, '/proInfo/saveProjectReply', function ajaxBack(data) {
            if (data.success) {
                thisO.parents(".comment_list").find(".replyNumO").text(data.attributes.replyNum);
                if(thisO.parents(".comment_list").find(".respond_box").length==0){
                    thisO.parents(".comment_list").find(".respond_div").append('<ul class="respond_box"></ul>')
                }
                thisO.parents(".comment_list").find(".respond_box").prepend('<li class="clearfix">'+
                '<div class="respond_l">'+
                '<div class="user_pic user_pic user_pic40">'+
                '<div class="user_border"></div>'+
