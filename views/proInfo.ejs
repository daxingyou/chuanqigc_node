    //点击回复，弹出回复框
    $(".comment_box").delegate(".reply_btn","click",function(){
        var postId=$(this).attr("postId");
        var replyForm='<textarea></textarea>'+
                      '<button class="btn_style reply_submit" postId='+postId+'>提交</button><button class="btn_style btn_disable ml_10 reply_cancel">取消</button>'
        $(this).next(".replydiv").empty().html(replyForm);

    })
