    function jianshaoshu(minNum) {
        var shu = document.getElementById('inp-num').value;
        var zengjiashu = parseInt(shu);
        if (zengjiashu > minNum) {
            zengjiashu = zengjiashu - 1;
        }
        document.getElementById('inp-num').value = zengjiashu;
//        minBuyPortionNum();
    }
    function zengjiashu(remainingShu) {
        var shu = document.getElementById('inp-num').value;
        var zengjiashu = parseInt(shu);
        if (zengjiashu < remainingShu) {
            zengjiashu = zengjiashu + 1;
        }
        document.getElementById('inp-num').value = zengjiashu;
    }
    function zengjiashuyr() {
        var shu = document.getElementById('inp-num').value;
        var zengjiashu = parseInt(shu);
        zengjiashu = zengjiashu + 1;
        document.getElementById('inp-num').value = zengjiashu;
    }

    function order(){
        if(!$("#inp-num").validationEngine("validate")){
            return false;
        }
        if(parseInt($("#inp-num").val())<1){
            dialog("请填写大于0的份数");
        }else if(parseInt($("#inp-num").val())>parseInt("<%- bigPart %>")){
            dialog("您输入的份数不在您可投份数内！");
        }else {
            var tzcount = document.getElementById("inp-num").value;
            var callbackUrl = "/paycenter/<%-projectId%>/" + '<%=proIndustry.projectName%>/' + tzcount;
            location.href = callbackUrl;
        }
    }

