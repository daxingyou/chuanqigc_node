            } else {
                dialog(data.msg);
            }
        });
    })

    //点击查看回复
    $(".comment_box").delegate(".look","click",function(){
        $(".momblack").css("height",$(document).height()).show();
        $("#lookComment_dialog").show();
        var postId=$(this).attr("postId");
        var pageNo=1;
        $("#lookComment_dialog").find(".respond_box").html("");
        replyList(postId,pageNo);
        //当滚动条滚到底部的事件
        $("#lookComment_dialog .dialog_form").scroll(function(){
            var $this =$(this),
                viewH =$(this).height(),//可见高度
                contentH =$(this).get(0).scrollHeight,//内容高度
                scrollTop =$(this).scrollTop();//滚动高度
            //if(contentH - viewH - scrollTop <= 100) { //到达底部100px时,加载新内容
            if(scrollTop/(contentH -viewH)>=0.95){ //到达底部100px时,加载新内容
                pageNo++;
                replyList(postId,pageNo);
            }
        });
    })
    function replyList(postId,pageNo){
        var data={
            "projectId": $("#projectId").val(),
            "postId":postId,
            "pageNo":pageNo
        }
        ajaxFunc(data, '/proInfo/getReplyList', function ajaxBack(data) {
            if (data.success) {
                data.attributes.projectRelayList.forEach(function(item,index){
                    $("#lookComment_dialog").find(".respond_box").append('<li class="clearfix">'+
                    '<div class="respond_l">'+
                    '<div class="user_pic  user_pic_40 user_pic40_white">'+
                    '<div class="user_border"></div>'+
                    '<img src="<%=picpath%>'+item.imageUrl+'">'+
                    '</div>'+
                    '</div>'+
                    '<div class="respond_r">'+
                    '<i>'+item.creater+'回复：</i>'+
                    '<em>'+item.content+'</em>'+
                    '<div class="comment_div comment_div2">'+
                    '<span>'+item.createtime+'</span>'+
                    '</div>'+
                    '</div>'+
                    '</li>')
                })

            } else {
                dialog(data.msg);
            }
        });
    }
    var totalRecords = '<%=postList.count%>'; // 总共条数
