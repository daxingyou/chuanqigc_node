    var picpath='<%=picpath%>';
    $("#paging").pagination({
        items: totalRecords,
        itemsOnPage: rowPerPage,
        cssStyle: 'light-theme',
        onPageClick: function(pageNumber){
            $.ajax({
                url: "/proInfo/getPostList",
                type: "post",
                dataType: "json",
                data: {"projectId": $("#projectId").val(),"pageNo":pageNumber},
                success: function (data) {
                    if(data){
