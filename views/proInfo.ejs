<script type="text/javascript">
    $(function () {

        $("#inp-num").validationEngine();
        //点击“查看”，弹出“查看评论”对话框
        box_dialog(".look","#lookComment_dialog");


        //点击”帮品牌“按钮，出现弹框
        box_dialog(".help_brand", "#bang_brand");
        //点击“帮品牌”弹框里的"单选项"，此项背景为红色，字体为白色
        $(".brand_radio_tab li").click(function(){
            $(this).addClass("brand_selected_tab").siblings().removeClass("brand_selected_tab");
        })
        //点击“帮品牌”弹框里的"多选项"，多项背景为红色，字体为白色
        $(".brand_check_tab li").click(function(){
            if($(this).hasClass("brand_selected_tab")){
                $(this).removeClass("brand_selected_tab");
            }else{
                $(this).addClass("brand_selected_tab");
            }
        })

        //点击“爱品牌”按钮，出现弹框
        box_dialog(".like_brand","#love_brand");
        //点击“爱品牌”，顶部介绍“小三角”，老板卡内容展开
        $(".love_trangle").click(function(){
            if($(".love_trangle").hasClass("up_trangle")){
                $(".bossCard_content").slideUp();
                $(this).removeClass("up_trangle");
            }else{
                $(".bossCard_content").slideDown();
                $(this).addClass("up_trangle");
            }
        });
        //项目信息---项目图片左右切换
        wind_control("#project_img");
        for(var i=1;i<'<%=industry.auditList.length%>'+1;i++){
            shh_infor("#tr"+i);
            enlarge("tr"+i);
        }
        //项目信息页---点击缩略图，图片放大；点击大图，图片变小
        enlarge1("project_img");

    })
    //预约认购
    $(".yuy_buy").click(function(){
        if(!$("#inp-num").validationEngine("validate")){
            return false;
        }
        if($("#inp-num").val()<1){
            dialog("请填写大于0的份数");
        }else{
            $(".momblack").show();
            $("#buy_dialog").show();
            $("#buyPart").html(parseInt($("#inp-num").val()));
            var totalMoney=(Math.round(($("#buyPart").text()*'<%-proIndustry.lowAmount%>')*100)/100);
            $("#totalMoney").html(totalMoney.toFixed(2));
        }
    })

    //帮品牌
    $("#helpBrandForm").submit(function (e) {
        e.preventDefault();
        var financialSupport = "";//资金支持
        var capabilitySupport="";//能力支持
        var myResources="";//资源支持

        $("#moneyUl li").each(function(){
            <%projectBrand.moneyHelp.forEach(function(item,index){%>
                if($(this).index()=="<%=index%>") {
                    $(this).attr("lid", "<%=item%>")
                }
            <%})%>
            if($(this).hasClass("brand_selected_tab")){
                var str=$(this).attr("lid");
                financialSupport=str.substring(0, str.length) + ",";
            }
        })
        $("#humanUl li").each(function(){
            <%projectBrand.humanHelp.forEach(function(item,index){%>
            if($(this).index()=="<%=index%>") {
                $(this).attr("lid", "<%=item%>")
            }
            <%})%>
            if($(this).hasClass("brand_selected_tab")) {
                var str = $(this).attr("lid");
                capabilitySupport += str.substring(0, str.length) + ",";
            }
        })
        $("#resourceUl li").each(function(){
            <%projectBrand.resourceHelp.forEach(function(item,index){%>
            if($(this).index()=="<%=index%>") {
                $(this).attr("lid", "<%=item%>")
            }
            <%})%>
            if($(this).hasClass("brand_selected_tab")) {
                var str = $(this).attr("lid");
                myResources += str.substring(0, str.length) + ",";
            }
        })
        financialSupport=financialSupport.substring(0,financialSupport.length-1);
        capabilitySupport=capabilitySupport.substring(0,capabilitySupport.length-1);
        myResources=myResources.substring(0,myResources.length-1);

        var data = {
            "projectId": $("#projectId").val(),
            "financialSupport": financialSupport,
            "capabilitySupport": capabilitySupport,
            "myResources": myResources,
            "content": $("#otherBang").val()//对项目的帮助内容
        }
        ajaxFunc(data, '/proInfo/saveOrUpdateProjectBangBrand', function ajaxBack(data) {
            if (data.success) {
                dialog("感谢您对我们的支持，您的帮品牌信息已经收到，我们会认真的审核，若通过，我们会尽快以短信或电话的形式与您联系，查看帮品牌进度，请进入个人中心-认购的项目-帮品牌，找到本项目即可查看！");
                $("#bang_brand").find(".module_close").click();
            } else {
                $('.error').html("<font color='red'>" + data.msg + "</font>");
            }
        });
    })
    $("#loveBrandForm").submit(function (e) {
        e.preventDefault();
        var cardNum="";//老板卡额度
        $("#funUl li").each(function(){
            if($(this).hasClass("brand_selected_tab")){
                var str= $.trim($(this).text());
                cardNum=str;
            }
        })
        if(cardNum==""){
            $('.error').html("<font color='red'>请选择老板卡额度</font>");
            return false;
        }
        var data = {
            "projectId": $("#projectId").val(),
            "cardNum": cardNum
        }
        ajaxFunc(data, '/proInfo/buyBossCard', function ajaxBack(data) {
            if (data.success) {
                dialog("<div style='line-height: 30px; text-align: left;'>　　老板卡购买成功，您可以在<a href='/usercenter/likebrand' style='color:#dd0000;'>用户中心</a>查看，另外，请核实您的收货地址，老板卡会在一周内寄送到您个人信息中的收货地址，请注意查收！</div>");
                $("#love_brand").find(".module_close").click();
            } else {
                $('.error').html("<font color='red'>" + data.msg + "</font>");
            }
        });
    })
    //预约购买
    $("#prebuy_btn").click(function () {
        if(!$("#inp-num").validationEngine("validate")){
            return false;
        }
        var data = {
            "projectId": $("#projectId").val(),
            count:$("#inp-num").val()
        }
        ajaxFunc(data, '/proInfo/saveOrder', function ajaxBack(data) {
            if (data.success) {
                $("#buy_dialog").find(".module_close").click();
                dialogLoad("预约购买成功！");
                var time=2;
                var t=setInterval(function(){
                    time--;
                    if(time==0){
                        $("#tips_dialog_load .module_close").click();
                        clearInterval(t);
                    }
                },1000)
            } else {
                dialog(data.msg)
            }
        });
    })

    //选项卡切换
    $(".tab_menu li").click(function () {
        $(".tab_menu li").attr("class", "");
        $(this).attr("class", "current");
        if($(this).index()==0){
            $(".tab_content .tab1").hide();
            $(".tab_content .tab1").eq(0).removeClass("hide");
            $(".tab_content .tab1").eq(0).show();
        }else{
            if('<%=locals.user%>'){
                $(".tab_content .tab1").hide();
                $(".tab_content .tab1").eq($(this).index()).removeClass("hide");
                $(".tab_content .tab1").eq($(this).index()).show();
            }else{
                $(".click_login").click();
            }
        }
    })

    function getCheckBoxValue(name){
        obj = document.getElementsByName(name);
        check_val = "";
        for(k in obj){
            if(obj[k].checked)
                check_val += obj[k].value +",";
        }
        return check_val;
    }

    //项目信息页、风控措施页图片左右无缝切换
    function wind_control(wind_dome) {
        var wind_lisum = $(wind_dome).find("li").length;
        if (wind_lisum > 4) {
            $(wind_dome).find(".plus").show();
            $(wind_dome).find(".minus").show();
            $(wind_dome).cxScroll({auto: false});
        } else {
            $(wind_dome).find(".plus").hide();
            $(wind_dome).find(".minus").hide();
        }
    }
    //审核信息----图片左右切换
    function shh_infor(shh_dome) {
        var shh_lisum = $(shh_dome).find("li").length;  //获取到每个list中，li的长度
        if (shh_lisum > 2) {   //如果li>2
            $(shh_dome).find(".plus").show();   //左右箭头显示
            $(shh_dome).find(".minus").show();
