                            '<span>'+item.createtime+'</span>'+
                            '<em class="reply look" postId="'+item.id+'">查看回复(<font class="replyNumO">'+item.replyNum+'</font>)</em>'+
                            '<em class="reply reply_btn" postId="'+item.id+'">回复</em>'+
                            '<div class="replydiv">'+
                            '</div>'+
                            '</div>'+
                            '<div class="respond_div">'+
                            '</div>'+
                            '</dd>'+
                            '</dl>'
                        });

                        $("#pageZone").html(str);

                    }else{
                        alert("传递格式有误")
                    }
                },
                error:function(){
                    alert("加载失败");
                }
            });
        }
    });
    function checkLength(which,maxChars) {
        if (which.value.length > maxChars)
        {
            dialog("您输入的字数超出限制!");
            // 超过限制的字数了就将 文本框中的内容按规定的字数 截取
            which.value = which.value.substring(0,maxChars);
            return false;
        }
        else
        {
            var curr = maxChars - which.value.length; //250 减去 当前输入的
            document.getElementByIdx_x("sy").innerHTML = curr.toString();
            return true;
        }
    }

